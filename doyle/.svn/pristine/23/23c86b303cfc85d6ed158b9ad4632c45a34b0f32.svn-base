        <script>
            var slideIndex = 1;
            showDivs(slideIndex);

            function plusDivs(n) {
                showDivs(slideIndex += n);
            }

            function showDivs(n) {
                var i;
                var x = document.getElementsByClassName("mySlides");
                if (n > x.length) {
                    slideIndex = 1
                }
                if (n < 1) {
                    slideIndex = x.length
                }
                for (i = 0; i < x.length; i++) {
                    x[i].style.display = "none";
                }
                x[slideIndex - 1].style.display = "block";
            }

            var slidestart = 1;
            show(slidestart);

            function plus(nn) {
                show(slidestart += nn);
            }

            function show(nn) {
                var ii;
                var xx = document.getElementsByClassName("Slide-1");
                if (nn > xx.length) {
                    slidestart = 1
                }
                if (nn < 1) {
                    slidestart = xx.length;
                }
                for (ii = 0; ii < xx.length; ii++) {
                    xx[ii].style.display = "none";
                }
                xx[slidestart - 1].style.display = "block";
            }
        </script>
